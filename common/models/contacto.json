{
  "name": "Contacto",
  "plural": "Contactos",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "email": {
      "type": "string",
      "required": true
    },
    "apellidos": {
      "type": "string",
      "required": true
    },
    "nombre": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "empresas": {
      "type": "belongsTo",
      "model": "Empresa",
      "foreignKey": "empresaId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "Empresa",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
