        <input name="nombre" type="text" ng-model="user.nombre" placeholder="Nombre de Usuario">
      </div>

      <div class="form-group">
        <label>Email</label>
        <input name="email" type="text" ng-model="user.email" placeholder="Email">
      </div>

      <div class="form-group">
        <label>Password</label>
        <input name="password" type="password" ng-model="user.password" placeholder="ContraseÃ±a">
      </div>

      <div class="actions">
        <label></label>
        <button>Sign up</button>
      </div>
    </fieldset>
  </form>
</section>
